    "def tower_of_hanoi (n,a,b,c):\n",
    "    if n == 1:\n",
    "        print(\"move 1st disk from\", a , \"to\", c)\n",
    "        return\n",
    "    tower_of_hanoi(n-1,a,c,b)\n",
    "    print(\"move\", n , \"th disk from\", a, \"to\", c)\n",
    "    tower_of_hanoi(n-1,b,a,c)\n",
    "\n",
    "\n",
    "n=int(input())\n",
    "tower_of_hanoi(n, 'a', 'b', 'c')"
   ]
